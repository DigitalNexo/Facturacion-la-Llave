{
  "name": "@fll/db",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "dotenv -e ../../.env -- prisma generate",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ../../.env -- prisma migrate reset",
    "studio": "dotenv -e ../../.env -- prisma studio",
    "seed": "dotenv -e ../../.env -- tsx src/seed.ts",
    "format": "prisma format"
  },
  "dependencies": {
    "@prisma/client": "^6.2.0"
  },
  "devDependencies": {
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "dotenv-cli": "^7.4.2"
  }
}
